{
  "name": "@hydro-project/hydroscope",
  "version": "1.0.0-alpha.7",
  "description": "React-based graph visualization library for Hydro dataflow programs",
  "keywords": [
    "hydro",
    "visualization",
    "graph",
    "react",
    "dataflow"
  ],
  "homepage": "https://github.com/hydro-project/hydroscope",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydro-project/hydroscope.git"
  },
  "license": "Apache-2.0",
  "type": "module",
  "author": "Hydro Project Contributors",
  "module": "dist/index.esm.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.esm.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./src/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "sideEffects": [
    "dist/style.css",
    "**/*.css"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "alpha"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "rollup -c",
    "build:clean": "npm run clean && npm run build",
    "dev": "rollup -c --watch",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:debug": "DEBUG_FILES=1 vitest run --reporter=verbose",
    "test:single": "vitest run --reporter=verbose",
    "test:match": "vitest run --reporter=verbose -t",
    "test:file": "vitest run --reporter=verbose",
    "test:reminder": "node -e \"if (process.env.ENABLE_TEST_LOGS !== 'true') console.log('\\nðŸ’¡ Tip: To see console.log output in tests, run with ENABLE_TEST_LOGS=true\\n   Example: ENABLE_TEST_LOGS=true npm test')\"",
    "posttest": "npm run test:reminder",
    "test:watch": "vitest --reporter=verbose",
    "test:performance": "vitest run --config vitest.performance.config.ts",
    "lint": "npm run lint:prod && npm run lint:tests",
    "lint:prod": "eslint 'src/**/*.{js,ts,tsx}' --ignore-pattern 'src/**/__tests__/**' --max-warnings 0",
    "lint:tests": "eslint 'src/__tests__/**/*.{js,ts,tsx}' --max-warnings 0",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "format:check": "prettier --check 'src/**/*.{js,ts,tsx}'",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "typecheck:strict": "tsc -p tsconfig.strict.json --noEmit",
    "dev-reports": "bash dev_reports/refresh.sh",
    "prepublishOnly": "npm run build:clean"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@xyflow/react": "^12.8.2",
    "antd": "^5.26.7",
    "elkjs": "^0.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^24.5.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "glob": "^11.0.3",
    "jsdom": "^26.1.0",
    "knip": "^5.62.0",
    "madge": "^8.0.0",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.28.0",
    "rollup-plugin-postcss": "^4.0.0",
    "ts-prune": "^0.10.3",
    "ts-remove-unused": "^0.1.0",
    "tslib": "^2.8.1",
    "tsr": "^1.3.4",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.28.0",
    "@rollup/rollup-darwin-x64": "^4.28.0",
    "@rollup/rollup-darwin-arm64": "^4.28.0",
    "@rollup/rollup-win32-x64-msvc": "^4.28.0"
  },
  "engines": {
    "node": ">=18.0"
  }
}
